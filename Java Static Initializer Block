import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    public static boolean flag = true;
    Scanner sc = new Scanner(System.in);
    int B = sc.nextInt();
    int H = sc.nextInt();
//    Write your code here

    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        if(flag){
            try {
                int B = sc.nextInt();
                int H = sc.nextInt();

                if (B <= 0 || H <= 0) {
                    throw new Exception("Breadth and height must be positive");
                }


                int area=B*H;
                System.out.print(area);
            }

            catch(Exception e){
                System.out.print("java.lang.Exception: " + e.getMessage());
            }
        }

    }//end of main

}//end of class

